@model MercadonaStudi.ViewModels.LoginViewModel

@{
    ViewData["Title"] = "Profil";
    Layout = "_Layout";
}

<section class="container-fluid">
    <div class="d-flex border-start border-bottom border-success-subtle ps-2 pt-0">
        <span class="border-start border-success pt-2 p-0"></span>
        <h1 class="border-bottom border-success p-1">Profil</h1>
    </div>

    <div class="form-group mt-3">
        <i class="bi bi-person-circle"></i>
        <h2>Bonjour Admin !</h2>
    </div>

    <div class="form-group mt-3">
        <label asp-for="Email" class="form-label">Email</label>
    </div>

    <div class="form-group mt-3">
        <label asp-for="Password" class="form-label">Mot de passe</label>
    </div>
</section>